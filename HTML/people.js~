		var p=new People("Petros","Petrosyan","programmer","armenian","Republic of Armenia");

	        function People (name_p,family_p,profession_p,nationality_p,residence_p) {
                this.name=name_p;
                this.family=family_p;
                this.name=profession_p;
                this.family=nationality_p;
                this.name=residence_p; 
                this.print=function(str)
                if (str=="name")
                alert(this.name);
                if (str=="family")
                alert(this.family);
                if (str=="age")
                alert(this.age);
                if (str=="nationality")
                alert(this.nationality);
                if (str=="residence")
                alert(this.residence);
        };

              
                              

